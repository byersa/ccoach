<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>Contact Details</title>
</head>
<body>
<div class="contactDetailsPage"
     data-dojo-type="dijit.layout.ContentPane"
     style="overflow: hidden; margin: 0 10px 0 10px;">

    <div class="dojoxGridRow">
        <table class="dojoxGridRowTable" border="0" cellspacing="0" cellpadding="0">
            <tbody>
                <tr>
                    <td class="dojoxGridCell dojoxGridCellRowHeader">
                        ID
                    </td>
                    <td class="dojoxGridCell">
                        ${0}
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="dojoxGridRow dojoxGridRowOdd">
        <table class="dojoxGridRowTable" border="0" cellspacing="0" cellpadding="0">
            <tbody>
            <tr>
                <td class="dojoxGridCell dojoxGridCellRowHeader">
                    Name
                </td>
                <td class="dojoxGridCell">
                    <div data-context-query=".contactNameTextBox"
                         data-dojo-props="value: '${2}', tabIndex: 1">
                    </div>
                </td>
            </tr>
            </tbody>
        </table>
    </div>
    <div class="dojoxGridRow">
        <table class="dojoxGridRowTable" border="0" cellspacing="0" cellpadding="0">
            <tbody>
            <tr>
                <td class="dojoxGridCell dojoxGridCellRowHeader">
                    Last Name
                </td>
                <td class="dojoxGridCell">
                    <div data-context-query=".contactLastNameTextBox"
                         data-dojo-props="value: '${3}', tabIndex: 2">
                    </div>
                </td>
            </tr>
            </tbody>
        </table>
    </div>
    <div class="dojoxGridRow dojoxGridRowOdd">
        <table class="dojoxGridRowTable" border="0" cellspacing="0" cellpadding="0">
            <tbody>
            <tr>
                <td class="dojoxGridCell dojoxGridCellRowHeader">
                    Address
                </td>
                <td class="dojoxGridCell">
                    ${4}
                </td>
            </tr>
            </tbody>
        </table>
    </div>
    <div class="dojoxGridRow">
        <table class="dojoxGridRowTable" border="0" cellspacing="0" cellpadding="0">
            <tbody>
            <tr>
                <td class="dojoxGridCell dojoxGridCellRowHeader">
                    Phone Number
                </td>
                <td class="dojoxGridCell">
                    ${5}
                </td>
            </tr>
            </tbody>
        </table>
    </div>
    <div class="dojoxGridRow dojoxGridRowOdd">
        <table class="dojoxGridRowTable" border="0" cellspacing="0" cellpadding="0">
            <tbody>
            <tr>
                <td class="dojoxGridCell dojoxGridCellRowHeader">
                    Email
                </td>
                <td class="dojoxGridCell">
                    ${6}
                </td>
            </tr>
            </tbody>
        </table>
    </div>
    <div class="dojoxGridRow">
        <table class="dojoxGridRowTable" border="0" cellspacing="0" cellpadding="0">
            <tbody>
            <tr>
                <td class="dojoxGridCell dojoxGridCellRowHeader">
                    Skype
                </td>
                <td class="dojoxGridCell">
                    ${7}
                </td>
            </tr>
            </tbody>
        </table>
    </div>
    <div class="dojoxGridRow dojoxGridRowOdd">
        <table class="dojoxGridRowTable" border="0" cellspacing="0" cellpadding="0">
            <tbody>
            <tr>
                <td class="dojoxGridCell dojoxGridCellRowHeader">
                    Offer Activity
                </td>
                <td class="dojoxGridCell">
                    ${8}
                </td>
            </tr>
            </tbody>
        </table>
    </div>
</div>
</body>
</html>